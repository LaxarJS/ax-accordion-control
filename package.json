{
  "name": "laxar-accordion-control",
  "version": "2.0.0-pre",
  "description": "Wraps the jQuery UI accordion as an AngularJS v1 directive, for LaxarJS widgets",
  "homepage": "http://laxarjs.org",
  "main": "laxar-accordion-control.js",
  "browser": "dist/laxar-accordion-control.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LaxarJS/ax-accordion-control.git"
  },
  "scripts": {
    "prepublish": "npm run dist",
    "test": "npm run karma && npm run eslint",
    "dist": "cross-env NODE_ENV=production webpack",
    "browser-spec": "npm run browser-spec-build && npm run browser-spec-start",
    "browser-spec-build": "cross-env NODE_ENV=browser-spec webpack",
    "browser-spec-start": "cross-env NODE_ENV=browser-spec webpack-dev-server --inline",
    "karma": "karma start karma.config.js",
    "eslint": "eslint *.js spec"
  },
  "dependencies": {},
  "devDependencies": {
    "angular": "1.5.x",
    "angular-mocks": "1.5.x",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "cross-env": "^3.2.3",
    "eslint": "^3.17.1",
    "jquery": ">=1.7.0",
    "jquery-ui": "^1.12.1",
    "karma": "^1.5.0",
    "laxar": "^2.0.0",
    "laxar-infrastructure": "^0.1.0",
    "lodash.debounce": "^4.0.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "peerDependencies": {
    "angular": "1.5.x",
    "jquery": ">=1.7.0",
    "jquery-ui": "^1.12.1",
    "laxar": "^2.0.0",
    "lodash.debounce": "^4.0.8"
  },
  "bugs": {
    "url": "https://github.com/LaxarJS/ax-accordion-control/issues",
    "email": "info@laxarjs.org"
